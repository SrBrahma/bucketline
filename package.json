{
  "name": "bucketline",
  "version": "0.0.1",
  "module": "index.ts",
  "type": "module",
  "files": [],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "tsw": "tsc --watch --noEmit",
    "format": "biome check --write --unsafe . && eslint --fix . package.json",
    "format:check": "biome check . && eslint . package.json",
    "pre-commit": "bun typecheck",
    "pre-commit2": "bun format && bun typecheck && bun test && git add -A",
    "test:watch": "bun test --watch",
    "prepare": "husky",
    "start": "bun i && bun run scripts/index.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@playwright/test": "^1.47.0",
    "@types/bun": "^1.1.8",
    "@types/node": "^22.5.4",
    "eslint-config-gev": "^4.5.1",
    "eslint-config-next": "^14.2.8",
    "eslint-plugin-valtio": "^0.6.4",
    "husky": "^9.1.5",
    "typescript": "^5.5.4"
  }
}
